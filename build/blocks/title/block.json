{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cil-blocks/title",
  "title": "Title",
  "category": "mp-blocks",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "selector": ".cil-title"
    },
    "showIcon": {
      "type": "boolean",
      "default": true
    },
    "size": {
      "type": "string",
      "default": "h2"
    }
  },
  "textdomain": "cil-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}