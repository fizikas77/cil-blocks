{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cil-blocks/heading",
  "title": "Heading",
  "category": "mp-blocks",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "selector": ".heading__title"
    },
    "badge": {
      "type": "string",
      "selector": ".heading__badge"
    },
    "description": {
      "type": "string",
      "selector": ".heading__description"
    },
    "imageURL": {
      "type": "string",
      "selector": ".heading__image img",
      "attribute": "src"
    },
    "imageAlt": {
      "type": "string",
      "selector": ".heading__image img",
      "attribute": "alt"
    },
    "imageID": {
      "type": "number"
    },
    "showIcon": {
      "type": "boolean"
    },
    "imageRight": {
      "type": "boolean"
    },
    "verticalAlign": {
      "type": "string"
    },
    "decorURL": {
      "type": "string",
      "selector": ".heading__decor img",
      "attribute": "src"
    },
    "decorAlt": {
      "type": "string",
      "selector": ".heading__decor img",
      "attribute": "alt",
      "default": ""
    },
    "decorID": {
      "type": "number"
    }
  },
  "textdomain": "cil-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}