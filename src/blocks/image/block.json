{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cil-blocks/image",
	"title": "Image for heading",
	"category": "mp-blocks",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"imageURL": {
			"type": "string",
			"selector": ".mp-image img",
			"attribute": "src"
		},
		"imageAlt": {
			"type": "string",
			"selector": ".mp-image img",
			"attribute": "alt",
			"default": ""
		},
		"imageID": {
			"type": "number"
		},
		"imageSize": {
			"type": "string"
		}
	},
	"textdomain": "cil-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
